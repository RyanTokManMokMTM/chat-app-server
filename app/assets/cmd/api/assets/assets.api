syntax = "v1"

import "desc/file.api"

info (
	title:   "chat-app-router"
	desc:    "A router for a chat-app"
	author:  "Jackson.tmm"
	email:   "ryantokmanmokmtm@hotmail.com"
	version: "v1"
)

type (
	HealthCheckResp {
		Resp string `json:"resp"`
	}
)

@server (
	group:  health
	prefix: /api/v1
)
service AssetsAPI {
	@handler HealthCheck
	post /ping returns (HealthCheckResp)
}

