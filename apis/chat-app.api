syntax = "v1"
import "./desc/user.api"
info(
	title: "chat-app-server"
	desc: "A server for a chat-app"
	author: "Jackson.tmm"
	email: "ryantokmanmokmtm@hotmail.com"
	version: "v1"
)

type (
	HealthCheckReq  struct{}
	HealthCheckResp {
		Resp string `json:"resp"`
	}
)

@server (
	group: health
	prefix: /api/v1
)
service  ChatApp {
	@handler HealthCheck
	get /ping(HealthCheckReq) returns (HealthCheckResp)
}